{
    "cSpell.spellCheckOnlyWorkspaceFiles": true,
    "cSpell.enabled": true,
    "cSpell.language": "pt-BR,en-US",
    "triggerTaskOnSave.on": false,
    "triggerTaskOnSave.tasks": {
        "build":[
            "main.tex"
        ],
    },
    "cSpell.ignorePaths": [
        ".git/objects",
        ".vscode",
        "./include.tex",
        "./ref.bib",
    ],
    "cSpell.ignoreRegExpList": [
        "CStyleComment",
        "%.*$"
    ],
    "texlab.forwardSearch.executable": "okular",
    "texlab.forwardSearch.args": [
        "--unique",
        "--noraise",
        "main.pdf#src:%l%f",
    ],
    "texlab.chktex.onEdit": false,
    "texlab.chktex.onOpenAndSave": false,
    "texlab.diagnostics.ignoredPatterns": [
        "inputenc package ignored",
        "There were undefined references",
        "Underfull \\\\hbox",
        "Interword spacing",
        "Command terminated with space",
        "Use either `` or '' as an alternative to",
        "You ought to remove spaces in front of punctuation",
        "Wrong length of dash may have been used",
        "Delete this space to maintain correct pagereferences.",
        "Intersentence spacing (.+) should perhaps be used"
    ],
